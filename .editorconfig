# EditorConfig is awesome: https://EditorConfig.org

# Top-most EditorConfig file
root = true

# Default settings
[*]
indent_style = space
indent_size = 4
charset = utf-8
end_of_line = lf
insert_final_newline = false
trim_trailing_whitespace = true

# Match all HTML files
[*.html]
indent_style = space

# Match all CSS files
[*.css]
indent_style = space

# Match all JavaScript files
[*.js]
indent_style = space
